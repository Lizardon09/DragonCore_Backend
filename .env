# Password for the 'elastic' user (at least 6 characters)
ELASTIC_PASSWORD=Croagunk1!

# Password for the 'kibana_system' user (at least 6 characters)
KIBANA_PASSWORD=Croagunk1!

# Version of Elastic products
ELASTIC_STACK_VERSION=8.1.2

# Version of Kibana products
KIBANA_STACK_VERSION=8.1.2

# Version of MSSQL products
MSSQL_SERVER_VERSION=2022-latest

# Version of POSTGRES products
POSTGRES_SERVER_VERSION=14.5

# Set to 'basic' or 'trial' to automatically start the 30-day trial
LICENSE=basic
#LICENSE=trial

# Port to expose Elasticsearch HTTP API to the host
ES_PORT=9200
#ES_PORT=127.0.0.1:9200

# Port to expose Kibana to the host
KIBANA_PORT=5601
#KIBANA_PORT=80

# Port to expose MSSQL to the host
MSSQL_PORT=1433

# Port to expose POSTGRES to the host
POSTGRES_PORT=8080

# Port to expose DragoncoreBackend to the host
DRAGONCORE_BACKEND_PORT=44314
#DRAGONCORE_BACKEND_PORT=90

# Increase or decrease based on the available host memory (in bytes)
MEM_LIMIT=1073741824

# Project namespace (defaults to the current folder name if not set)
COMPOSE_PROJECT_NAME=dragoncore_backend